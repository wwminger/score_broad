on:
  push:
    branches:
    - main
    - release
jobs:
  publish-tauri:
    strategy:
      fail-fast: false
      matrix:
        platform: [macos-latest, ubuntu-latest, windows-latest]
    runs-on: ${{ matrix.platform }}
    steps:
    - name: tauri-build
      # You may pin to the exact commit or the version.
      # uses: JonasKruckenberg/tauri-build@83c379213090189243e77e5e5919c238efb435cc
      uses: JonasKruckenberg/tauri-build@v0.1.1

